<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
	"http://struts.apache.org/dtds/struts-2.1.dtd">
	
<!-- 钦治负责模块的Struts配置文件 -->

<struts>
<package name="advertise" extends="struts-default" namespace="/">
		<action name="addAdvertise" class="advertiseAction" method="addAdvertise">
		</action>
		<action name="toUnFinAdvertise" class="advertiseAction" method="toUnFinAdvertise">
			 <result name="toUnFinAdvertise">/postjob/weifabujob.jsp</result>
		</action>
		<action name="toUpdateAdvertise" class="advertiseAction" method="toUpdateAdvertise">
			 <result name="toUpdateAdvertise">/postjob/dashboard-post-a-job.jsp</result>
		</action>
		<action name="deleteAdvertise" class="advertiseAction" method="deleteAdvertise">
		</action>
		<action name="updateAdvertise" class="advertiseAction" method="updateAdvertise">
		</action>
		<action name="toFinAdvertise" class="advertiseAction" method="toFinAdvertise">
			 <result name="toFinAdvertise">/postjob/dashboard-manage-jobs.jsp</result>
		</action>
		<action name="detailAdvertise" class="advertiseAction" method="detailAdvertise">
			 <result name="detailAdvertise">/postjob/single-job-page.jsp</result>
		</action>
		<action name="toAdd" class="advertiseAction" method="toAdd">
			 <result name="toAdd">/postjob/dashboard-post-a-job.jsp</result>
		</action>
	</package>
	</struts>