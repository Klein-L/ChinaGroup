<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
	"http://struts.apache.org/dtds/struts-2.1.dtd">
	
<!-- 世平负责模块的Struts配置文件 -->

<struts>
	<package name="cv" extends="struts-default" namespace="/">
		<action name="cv_*" class="cvAction" method="{1}">
			<result name="show">/user/userCenter.jsp</result>
			<result name="add">/cv/resume.jsp</result>
			<result name="showResume">/cv/showResume.jsp</result>
			<result name="showUpdate">/cv/updateResume.jsp</result>
		<!-- 	<result name="delete">/cv/resume.jsp</result> -->
			<result name="input">/cv/resume.jsp</result>
			<result name="error">/cv/updateResume.jsp</result>
		</action>
	
	</package>
	
	
</struts>